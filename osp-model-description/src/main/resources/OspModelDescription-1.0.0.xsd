			<xs:element name="Variable" type="osp:VariableType" minOccurs="2" maxOccurs="2"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Arbitrary name which is used for referring to this variable group in connections. Variable group names must be unique for a given OspModelDescription.xml </xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="NmeaMwvType">
