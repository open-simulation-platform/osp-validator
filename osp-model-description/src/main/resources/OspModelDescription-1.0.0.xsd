			<xs:element name="NmeaTrueHeading" type="osp:NmeaTrueHeadingType"/>
			<xs:element name="NmeaStatus" type="osp:NmeaStatusType"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required">
