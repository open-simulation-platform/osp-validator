	<xs:complexType name="LinearMechanicalPortType">
		<xs:annotation>
			<xs:documentation>This group must contain exactly 1 Force and exactly 1 LinearVelocity, where the causalities of the two groups must be opposite of each other. The xml schema validates that the element contains the correct child elements, and the OSP-IS validator verifies the causalities.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Force" type="osp:ForceType"/>
			<xs:element name="LinearVelocity" type="osp:LinearVelocityType"/>
