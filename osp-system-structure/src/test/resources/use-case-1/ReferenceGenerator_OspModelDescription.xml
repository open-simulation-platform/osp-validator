			<LinearDisplacement name="linearPositionReference">
				<Variable ref="x_ref"/>				
				<Variable ref="y_ref"/>				
			</LinearDisplacement>
			<AngularDisplacement name="angularPositionReference">
				<Variable ref="psi_ref"/>				
